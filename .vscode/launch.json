  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/test",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb", // Use "gdb" if you're on Linux
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb/lldb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}

